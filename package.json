{
  "name": "pg-vitepress-tailwindcss",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://pg-vitepress-tailwindcss.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-vitepress-tailwindcss.git"
  },
  "scripts": {
    "dev": "vitepress dev docs  --host",
    "now": "npm run build && npm run preview",
    "lighthouse": "npx unlighthouse --no-cache --site https://pg-vitepress-tailwindcss.netlify.app",
    "build": "vitepress build docs",
    "preview": "vitepress serve docs",
    "typecheck": "vue-tsc --noEmit",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix ./",
    "format": "prettier --write \"**/*.{html,vue,svelte,astro,js,jsx,cjs,mjs,ts,tsx,cts,mts,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "engines": {
    "node": "^14.18 || >= 16.0.0"
  },
  "dependencies": {
    "@unhead/schema": "^1.11.15",
    "@unhead/schema-org": "^1.11.15",
    "@unhead/vue": "^1.11.15",
    "@vueuse/core": "^12.4.0",
    "defu": "^6.1.4",
    "lite-youtube-embed": "^0.3.3",
    "ofetch": "^1.4.1",
    "pinia": "^2.3.0",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.294",
    "@nuxt/eslint-config": "^0.7.5",
    "@pinegrow/tailwindcss-plugin": "^3.0.35",
    "@pinegrow/vite-plugin": "^3.0.69",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.5",
    "@unocss/preset-icons": "^65.4.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7",
    "unocss": "^65.4.0",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vitepress": "^1.5.0",
    "vue-tsc": "^2.2"
  },
  "web-types": "web-types.json"
}
